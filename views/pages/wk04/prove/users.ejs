<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ("../../../partials/head.ejs") %>
</head>

<body>
    <%- include ("../../../partials/header.ejs") %>
    <%- include ("../../../partials/nav.ejs") %>
    <main>
        <%- include ("../../../partials/store/nav.ejs") %>
        <section>
            <h2>Here is a list of the current users</h2>
            <table>
              <tr>
                <th>Id</th>
                <th>First</th>
                <th>Last</th>
                <th>Email</th>
              </tr>
              <% users.forEach(user => { %>
                <tr>
                  <td><%= user.id %></td>
                  <td><%= user.first %></td>
                  <td><%= user.last %></td>
                  <td><%= user.email %></td>
                </tr>
              <% }) %>
            </table>
        </section>
    </main>
    <%- include ("../../../partials/footer.ejs") %>
</body>
</html>